# Production environment example for Laravel
# Copy this to your server as `.env` and update values

APP_NAME="Supply System"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=http://your-domain.example

LOG_CHANNEL=stack
LOG_LEVEL=info

# Database (Hostinger default MySQL)
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=u504712036_system_manage
DB_USERNAME=u504712036_root
DB_PASSWORD=

# Cache / Sessions / Queue
BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=public
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

# Mail (update to your provider)
MAIL_MAILER=smtp
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USERNAME=
MAIL_PASSWORD=
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=no-reply@your-domain.example
MAIL_FROM_NAME="${APP_NAME}"
